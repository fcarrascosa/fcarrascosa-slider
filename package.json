{
  "name": "fcarrascosa-slider",
  "version": "0.6.2",
  "description": "A simple slider based in web components via lit-element",
  "main": "index.js",
  "scripts": {
    "start": "polymer analyze > analysis.json && polymer serve -o",
    "test": "karma start",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "build": "polymer build",
    "test:watch": "karma start --auto-watch=true --single-run=false",
    "test:es5": "karma start karma.es5.config.js",
    "test:es5:watch": "karma start karma.es5.config.js --auto-watch=true --single-run=false",
  },
  "keywords": [
    "lit-element",
    "lit-html",
    "javascript",
    "slider"
  ],
  "author": "Fernando Carrascosa",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/fcarrascosa/fcarrascosa-slider"
  },
  "dependencies": {
    "lit-element": "^2.0.1"
  },
  "devDependencies": {
    "@open-wc/testing": "^0.9.0",
    "@open-wc/testing-karma": "^0.4.0",
    "@open-wc/testing-karma-bs": "^0.2.0",
    "@polymer/iron-component-page": "^4.0.1",
    "@polymer/iron-demo-helpers": "^3.0.2",
    "@polymer/iron-pages": "^3.0.1",
    "@webcomponents/webcomponentsjs": "^2.2.7",
    "chai": "^4.2.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.16.0",
    "karma-mocha": "^1.3.0",
    "karma-sinon-chai": "^2.0.2",
    "mocha": "^5.2.0",
    "polymer-cli": "^1.9.6",
    "sinon": "^7.2.3",
    "sinon-chai": "^3.3.0",
    "webpack-merge": "^4.1.5"
